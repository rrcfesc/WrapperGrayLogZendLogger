{
    "name": "rioxygen/wrappergraylogzendlogger",
    "description": "Adapter de Zend\/Logger\/Logger" ,
    "license": "BSD 3-Clause License",
    "keywords": ["wrapper","graylog","logger"],
    "authors": [
        {
            "name": "Ricardo Ruiz",
            "email": "ricardojesus.ruizcruz@gmail.com"
        }
    ],
    "require":{
        "php"                       : ">=5.6",
        "zendframework/zend-log"    : "2.*",
        "graylog2/gelf-php"         : "1.*",
        "psr/log-implementation"    : "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "php-coveralls/php-coveralls": "dev-master",
        "phpunit/phpcov": "^2.0",
        "squizlabs/php_codesniffer": "1.5.5"
    },
    "autoload": {
        "psr-4": {
            "Rioxygen\\Graylog\\": "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
                "Rioxygen\\Graylog\\" : "tests/"
        }
    }
}
